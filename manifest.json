  },
    
    "sandbox": {
    "pages": ["apps/tunnel-rush/index.html","apps/mc-188/index.html","apps/mc-152/index.html","apps/tanktrouble2/index.html"]
  },
    
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png",
    "180": "images/icon180.png"
  }
}